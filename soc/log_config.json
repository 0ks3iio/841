    "verbose": {
      "format": "%(asctime)s %(levelname)s %(module)s %(process)d %(thread)d %(filename)s:%(lineno)s %(message)s",
      "datefmt": "%Y-%m-%d %H:%M:%S"
    },
    "simple": {
      "format": "%(asctime)s %(levelname)s %(message)s",
      "datefmt": "%Y-%m-%d %H:%M:%S"
    }
  },
